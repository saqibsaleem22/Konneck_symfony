{% extends 'base.html.twig' %}
{% block stylesheets %}
<style>
    p {
        font-size: 20px;
    }
    div {
        width: 500px;
        margin: auto !important;
        margin-top: 20px !important;
        text-align: center;
    }
</style>

{% endblock %}
{% block body %}
<div style="margin: 100px; border: 1px solid black">
    <img width="200px" height="200px" src="{{ userOther.userAvatar }}" alt="">
    <h3>Name:</h3><p>{{ userOther.name }}</p>
    <h3>City:</h3><p>{{ userOther.userCity }}</p>
    <h3>Gender:</h3><p>{{ userOther.userGender }}</p>
</div>

{% endblock %}